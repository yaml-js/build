    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/uuid": "^10",
    "@types/yaml": "^1.9.7",
    "@vercel/ncc": "^0.38.1",
    "esbuild": "^0.23.0",
    "esbuild-node-externals": "^1.14.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "uuid": "^10.0.0"
  },
  "dependencies": {

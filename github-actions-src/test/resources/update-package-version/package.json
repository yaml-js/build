{
  "name": "main",
  "packageManager": "yarn@4.4.0",
  "version": "${FROM TAG}",
  "scripts": {
    "lint": "eslint ./eslint.config.js",
    "format": "prettier --config ./prettier.config 'src/**/*.ts' --write",
    "test": "jest --collect-coverage",
